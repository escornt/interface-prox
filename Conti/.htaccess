RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d


#RewriteRule ^centres-de-montage.html ps/index.php [QSA]
RewriteRule ^([^/]*)/$ /index.php?controller=$1&action=index [L]
RewriteRule ^([^/]*)/([^/]*)/$ /index.php?controller=$1&action=$2 [L]
RewriteRule ^([^/]*)/([^/]*)/([^/]*)/$ /index.php?controller=$1&action=$2&param1=$3 [L]





